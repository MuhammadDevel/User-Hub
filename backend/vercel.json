{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
      {
        "src": "/api/user/register",
        "dest": "index.js" 
      },
      {
        "src": "/api/user/login",
        "dest": "index.js" 
      },
      {
        "src": "/auth/callback", 
        "dest": "index.js" 
      }
  ]
}
